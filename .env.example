# Tree Re-ID API Configuration
# Copy to .env and edit as needed. All vars prefixed TREEID_.

TREEID_DATABASE_URL=postgresql+asyncpg://treeid:treeid@db:5432/treeid
TREEID_MODEL_CONFIG_PATH=/models/model_config.yaml
TREEID_PHOTO_STORAGE_PATH=/photos
TREEID_IMAGE_BASE_PATH=/images

# Inference defaults
TREEID_DEFAULT_GPS_RADIUS_M=15.0
TREEID_DEFAULT_TOP_K=5
TREEID_DEFAULT_ALPHA=0.9
TREEID_N_PROTOTYPES=3
TREEID_OUTLIER_THRESHOLD=0.5

# CORS - comma-separated origins, or ["*"] for all
# TREEID_CORS_ORIGINS=["http://localhost:3000","https://myapp.com"]
